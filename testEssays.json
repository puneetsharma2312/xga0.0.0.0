				"name": "invoice_item",
				"collection": "invoice_items",
				"find_one": true,
				"filter": "{ \"_id\": \":itemId\" }",
				"options": "{}"
			},
			{
				"name": "current_user_data",
				"collection": "users",
				"find_one": true,
				"filter": "{ \"_id\": \"Meteor.userId()\" }",
				"options": "{}"
