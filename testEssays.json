						"exportable": true
					},
					{
						"name": "phone",
						"title": "Phone",
						"type": "string",
						"default": "-",
						"exportable": true
					},
					{
						"name": "email",
						"title": "E-mail",
						"type": "email",
						"exportable": true
					},
					{
						"name": "note",
						"title": "Note",
						"type": "string",
						"exportable": true,
						"input": "textarea",
						"show_in_dataview": false
					},
					{
						"name": "totalAmount",
						"title": "Invoiced",
						"type": "float",
						"exportable": true,
						"show_in_insert_form": false,
						"show_in_update_form": false
					}
				],
				"owner_field": "ownerId",
				"roles_allowed_to_read": [
					"owner"
				],
				"roles_allowed_to_update": [
					"owner"
				],
				"roles_allowed_to_delete": [
					"owner"
				]
			},
			{
				"name": "invoices",
				"type": "collection",
				"fields": [
					{
						"name": "invoiceNumber",
						"title": "Invoice number",
						"type": "string",
						"required": true,
						"exportable": true
					},
					{
						"name": "date",
						"title": "Invoice date",
						"type": "date",
						"default": "today",
						"required": true,
						"format": "MM-DD-YYYY",
						"exportable": true,
						"input": "datepicker"
					},
					{
						"name": "customerId",
						"title": "Customer",
						"type": "string",
						"required": true,
						"input": "select",
						"lookup_query_name": "customer_list",
						"lookup_key": "_id",
						"lookup_field": "name",
						"join_collection": "customers",
						"join_container": "customer",
						"join_fields": [
							"name"
						],
						"show_in_dataview": false,
						"show_in_read_only_form": false
					},
					{
						"name": "customer.name",
						"title": "Customer",
						"type": "string",
						"exportable": true,
						"show_in_insert_form": false,
						"show_in_update_form": false
					},
					{
						"name": "totalAmount",
						"title": "Total",
						"type": "float",
						"exportable": true,
						"show_in_insert_form": false,
						"show_in_update_form": false
					}
				],
				"owner_field": "ownerId",
				"roles_allowed_to_read": [
					"owner"
				],
				"roles_allowed_to_update": [
					"owner"
				],
				"roles_allowed_to_delete": [
					"owner"
				],
				"before_insert_code": "if(!doc.totalAmount) doc.totalAmount = 0;"
			},
			{
				"name": "invoice_items",
				"type": "collection",
				"fields": [
					{
						"name": "description",
						"title": "Description",
						"type": "string",
						"required": true,
						"exportable": true
					},
					{
						"name": "quantity",
						"title": "Quantity",
						"type": "float",
						"default": 1,
						"required": true,
						"exportable": true
					},
					{
						"name": "price",
						"title": "Price",
						"type": "float",
						"required": true,
						"exportable": true
					},
					{
						"name": "amount",
						"title": "Amount",
						"type": "float",
						"exportable": true,
						"show_in_insert_form": false,
						"show_in_update_form": false
					}
				],
				"owner_field": "ownerId",
				"roles_allowed_to_read": [
					"owner"
				],
				"roles_allowed_to_update": [
					"owner"
				],
				"roles_allowed_to_delete": [
					"owner"
				],
				"before_insert_code": "doc.amount = doc.quantity * doc.price;",
				"before_update_code": "if(!modifier.$set) return; var quantity = modifier.$set.quantity || doc.quantity; var price = modifier.$set.price || doc.price; modifier.$set.amount = quantity * price;",
				"after_insert_code": "var sum = 0; InvoiceItems.find({ invoiceId: doc.invoiceId }).map(function(item) { sum += item.amount; }); Invoices.update({ _id: doc.invoiceId }, { $set: { totalAmount: sum }});",
				"after_update_code": "var sum = 0; InvoiceItems.find({ invoiceId: doc.invoiceId }).map(function(item) { sum += item.amount; }); Invoices.update({ _id: doc.invoiceId }, { $set: { totalAmount: sum }});",
				"after_remove_code": "var sum = 0; InvoiceItems.find({ invoiceId: doc.invoiceId }).map(function(item) { sum += item.amount; }); Invoices.update({ _id: doc.invoiceId }, { $set: { totalAmount: sum }});"
			},
			{
				"name": "essays",
				"type": "collection",
				"fields": [
					{
						"name": "title",
